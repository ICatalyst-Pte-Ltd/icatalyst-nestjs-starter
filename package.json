{
  "name": "icatalyst-nestjs-starter",
  "version": "1.0.0",
  "description": "A NestJS starter template for ICatalyst projects",
  "author": "Christopher Zhong",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.ts",
    "prepare": "node --eval \"try { require('husky').install() } catch (error) { if (error.code !== 'MODULE_NOT_FOUND') throw error }\""
  },
  "engines": {
    "node": "^18.0.0 || ^20.0.0"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.17.14",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^10.4.20",
    "@nestjs/core": "^11.1.11",
    "@nestjs/elasticsearch": "^10.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/websockets": "^11.1.11",
    "@prisma/client": "^5.19.1",
    "ajv": "^8.12.0",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^4.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "passport-jwt": "^4.0.1",
    "prisma": "^5.19.1",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.13.0",
    "typescript-eslint": "^8.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^11.1.11",
    "@semantic-release/gitlab-config": "^13.0.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^18.19.50",
    "@types/passport-jwt": "^3.0.13",
    "@types/sanitize-html": "^2.13.0",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-mock-extended": "^3.0.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "semantic-release": "^22.0.12",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  }
}
